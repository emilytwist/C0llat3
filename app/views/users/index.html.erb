<table class="table table-hover">
	<tr>
		<th>First Name</th>
		<th>Last Name</th>
		<th>Company</th>
		<th>Email</th>
	</tr>
<% @users.each do |user| %>
	<tr>
		<td><%= user.first_name %></td>
		<td><%= user.last_name %></td>
		<td><%= user.company %></td>
		<td><%= user.email %></td>
	</tr>
<% end %>
</table>